{
  "outputDirectory": "dist",
  "rewrites": [
    { "source": "/images/(.*)", "destination": "/images/$1" },
    { "source": "/assets/(.*)", "destination": "/assets/$1" },
    { "source": "/mbti", "destination": "/mbti/index.html" },
    { "source": "/mbti/(.*)", "destination": "/mbti/index.html" }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
